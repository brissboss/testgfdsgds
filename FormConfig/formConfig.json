{
    "Blockchain": {
        "icon": "ri-btc-line",
        "input": {
            "divider_1": {
                "type": "divider",
                "label": "Informations générales"
            },
            "input_1": {
                "type": "text",
                "label": "Nom du projet",
                "name": "name",
                "required": true,
                "editable": true,
                "rules": [
                    "required"
                ]
            },
            "input_2": {
                "type": "number",
                "label": "Prix d'un token",
                "name": "price",
                "required": true,
                "info": "Le prix d'un token est le prix d'une part du projet que vous souhaitez vendre",
                "rules": [
                    "required",
                    "upper:0",
                    "priceFormatter"
                ]
            },
            "input_3": {
                "type": "number",
                "label": "Nombre de tokens",
                "name": "total_supply",
                "required": true,
                "info": "Le nombre de tokens est le nombre de parts du projet que vous souhaitez vendre",
                "rules" : [
                    "required",
                    "upper:0",
                    "integer"
                ]
            },
            "input_4": {
                "type": "text",
                "label": "Symbole du token",
                "name": "symbol",
                "required": true,
                "info": "Le symbole du token est un identifiant unique qui permet de différencier les tokens entre eux. Il est généralement composé de 3 à 5 caractères.",
                "rules": [
                    "required"
                ]
            },
            "input_5": {
                "type": "text",
                "label": "Nombre maximal de tokens par client",
                "name": "max_authorized_quantity",
                "required": false,
                "info": "Le nombre maximal de tokens par client est le nombre maximal de tokens qu'un client peut acheter. Si vous ne souhaitez pas limiter le nombre de tokens par client, laissez ce champ vide.",
                "rules": [
                    "integer"
                ]
            }
        }
    },
    "Aperçu": {
        "icon": "ri-image-2-line",
        "input": {
            "divider_1": {
                "type": "divider",
                "label": "Ajouter vos images et descriptifs"
            },
            "input_1": {
                "type": "iframe",
                "label": "https://vuetifyjs.com/en/"
            }
        }
    },
    "Informations": {
        "icon": "ri-information-line",
        "input": {
            "divider_1": {
                "type": "divider",
                "label": "Point fort du projet"
            },
            "input_1": {
                "type": "textArea",
                "label": "1er point fort",
                "name": "strongPoint1",
                "required": true,
                "info": "Listez un fort du projet",
                "editable": true
            },
            "input_2": {
                "type": "textArea",
                "label": "2ème point fort",
                "name": "strongPoint2",
                "required": true,
                "info": "Listez un fort du projet",
                "editable": true
            },
            "input_3": {
                "type": "textArea",
                "label": "3ème point fort",
                "name": "strongPoint3",
                "required": true,
                "info": "Listez un fort du projet",
                "editable": true
            },
            "divider_2": {
                "type": "divider",
                "label": "Caractéristiques du bien",
                "name": "divider_2"
            },
            "input_5": {
                "type": "number",
                "label": "Surface en m²",
                "name": "surface",
                "required": true,
                "editable": true
            },
            "input_6": {
                "type": "number",
                "label": "Nombre de salle de bain",
                "name": "bathroom",
                "required": true,
                "editable": true
            },
            "input_7": {
                "type": "number",
                "label": "Capacité d'accueil",
                "name": "capacity",
                "required": true,
                "editable": true
            },
            "input_8": {
                "type": "number",
                "label": "Nombre de chambre",
                "name": "bedroom",
                "required": true,
                "editable": true
            },
            "input_9": {
                "type": "textArea",
                "label": "Description du bien",
                "name": "description",
                "required": true,
                "editable": true
            },
            "divider_3": {
                "type": "divider",
                "label": "Localisation du bien"
            },
            "input_11": {
                "type": "text",
                "label": "Adresse",
                "name": "address",
                "required": true,
                "editable": true
            },
            "input_12": {
                "type": "text",
                "label": "Ville",
                "name": "city",
                "required": true,
                "editable": true
            },
            "input_13": {
                "type": "number",
                "label": "Code postal",
                "name": "zipCode",
                "required": true,
                "editable": true
            }
        }
    }
}